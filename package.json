{
  "name": "gpg-electron",
  "productName": "DesktopGPG",
  "version": "1.0.0",
  "description": "A minimal GPG Electron application",
  "repository": "tech-slayers/desktopgpg",
  "author": "TechSlayers <support@techslayers.com>",
  "license": "CC0-1.0",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "main": "main.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "makea": "electron-forge make --platform darwin && electron-forge make --platform linux && electron-forge make --platform win32",
    "makem": "electron-forge make --platform mas",
    "makew32": "electron-forge make --platform win32 --arch ia32"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.54",
    "@electron-forge/maker-deb": "^6.0.0-beta.54",
    "@electron-forge/maker-rpm": "^6.0.0-beta.54",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.54",
    "@electron-forge/maker-zip": "^6.0.0-beta.54",
    "electron": "^12.0.0"
  },
  "dependencies": {
    "bootstrap": "^4.6.0",
    "electron-squirrel-startup": "^1.0.0",
    "electron-util": "^0.14.2",
    "fs-extra": "^9.1.0",
    "jquery": "^3.6.0",
    "openpgp": "^5.0.0-1",
    "popper.js": "^1.16.1"
  },
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "DesktopGPG"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  }
}
